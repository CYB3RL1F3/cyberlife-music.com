{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production pnpm remix-serve build/index.js",
    "build": "pnpm build:css && pnpm build:worker && pnpm build:remix",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/styles.css",
    "build:remix": "pnpm remix build",
    "build:worker": "ls -la ./app && esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev": "npm-run-all -p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/styles.css",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "download:schema": "apollo service:download --endpoint=http://localhost:4001/v2/graphql ./graphql-schema.json",
    "clean:types": "npx ts-node ./scripts/fix-type-files.ts",
    "generate:component": "npx ts-node ./scripts/create-component.ts",
    "generate:graphql": "pnpm codegen && pnpm clean:types",
    "codegen": "pnpm download:schema && graphql-codegen --config ./codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^4.0.0",
    "@esbuild/darwin-x64": "0.25.9",
    "@hookform/resolvers": "^5.2.1",
    "@paypal/react-paypal-js": "^8.8.3",
    "@remix-run/dev": "^2.17.0",
    "@remix-run/node": "^2.17.0",
    "@remix-run/react": "^2.17.0",
    "@remix-run/serve": "^2.17.0",
    "@remix-run/server-runtime": "^2.17.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@wry/context": "^0.7.4",
    "@wry/equality": "^0.5.7",
    "@wry/trie": "^0.5.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^4.2.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "esbuild": "0.25.9",
    "framer-motion": "^12.23.12",
    "graphql": "16.11.0",
    "graphql-tag": "2.12.6",
    "html-react-parser": "^5.2.6",
