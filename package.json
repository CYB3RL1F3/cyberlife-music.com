    "node-persist": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "optimism": "^0.18.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.17.0",
    "react": "^19.1.1",
    "react-audio-player": "^0.17.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-hooks-form": "^2.0.1",
    "react-icons": "^5.5.0",
    "react-map-gl": "^8.0.4",
